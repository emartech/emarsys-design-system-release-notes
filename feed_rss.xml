---
layout: null
---

<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">

    <channel>
        <title>{{ site.title }}</title>
        <link>https://redirector.eservice.emarsys.net/ui/latest/doc/release-notes.html</link>
        <description>{{ site.description }}</description>
        <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
        {% for post in site.posts %}
        <item>
			<title>
				{{ post.title }}
			</title>
            <description>
                {{ post.content | escape }}
            </description>
            <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
            <guid>
                https://redirector.eservice.emarsys.net/ui/latest/doc/release-notes.html#{{ post.id | uri_escape }}
            </guid>
        </item>
        {% endfor %}
    </channel>

</rss>
